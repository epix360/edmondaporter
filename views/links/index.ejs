<% layout('/layouts/boilerplate') %>
    <h1>Links</h1>
    <div class="mb-3">
        <% for (let link of links) { %>
            <%- link.content %>
                <% } %>
    </div>
    <% if(currentUser && profile.equals(currentUser._id)) { %>
        <div class="mb-3">
            <a class="btn btn-info" href="/links/<%=link._id%>/edit">Edit Link List</a>
        </div>
        <% } %>